  <section-def-macros>
    <def-macro n="fixup1" npar="1">
      <choose>
        <when>
          <test><and><equal><clip pos="1" side="tl" part="a_der"/><lit-tag v="der_s"/></equal>
                     <equal><clip pos="1" side="tl" part="a_cmp"/><lit v=""/></equal></and></test>
          <let><clip pos="1" side="tl" part="a_der"/><lit v=""/></let>
        </when>
      </choose>

      <choose>
        <when>
          <test><equal><clip pos="1" side="tl" part="a_pos"/><lit-tag v="foc_kin"/></equal></test>
          <let><clip pos="1" side="tl" part="a_pos"/><lit-tag v="adv"/></let>
        </when>
      </choose>
      <choose>
        <when>
          <test><equal><clip pos="1" side="tl" part="a_cas"/><lit-tag v="CD"/></equal></test>
          <let><clip pos="1" side="tl" part="a_cas"/><lit-tag v="nom"/></let>
        </when>
      </choose>
      <choose>
        <when>
          <test><equal><clip pos="1" side="tl" part="a_nbr"/><lit-tag v="ND"/></equal></test>
          <let><clip pos="1" side="tl" part="a_nbr"/><lit-tag v="sg"/></let>
        </when>
      </choose>
      <let><clip pos="1" side="tl" part="a_cmp_feil"/><lit-tag v="sg"/></let>
      <let><clip pos="1" side="tl" part="a_cmp_only"/><lit v=""/></let> <!-- Ok while we don't treat compounds -->
      <let><clip pos="1" side="tl" part="a_kill"/><lit v=""/></let>
      <let><clip pos="1" side="tl" part="a_sem"/><lit v=""/></let> <!-- Should be fixed upstream -->
    </def-macro>
  </section-def-macros>

  <section-rules>
    
    <rule comment=""> 
      <pattern>
        <pattern-item n="unknown"/>
      </pattern>
      <action>
        <call-macro n="fixup1"><with-param pos="1"/></call-macro>
        <out>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
        </out>
      </action>
    </rule>
     
    <rule comment=""> 
      <pattern>
        <pattern-item n="default"/>
      </pattern>
      <action>
        <call-macro n="fixup1"><with-param pos="1"/></call-macro>
        <out>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
        </out>
      </action>
    </rule>
     
    <rule comment=""> 
      <pattern>
        <pattern-item n="unknown"/>
        <pattern-item n="clit"/>
      </pattern>
      <action>
        <choose>
          <when>
            <test><or><equal caseless="yes"><clip pos="2" side="tl" part="lem"/><lit v="kin"/></equal>
                      <equal caseless="yes"><clip pos="2" side="tl" part="lem"/><lit v="kaan"/></equal></or></test>
            <let><clip pos="2" side="tl" part="whole"/>
                 <concat><get-case-from pos="2"><lit v="myÃ¶s"/></get-case-from><lit-tag v="adv"/></concat></let>
          </when>
        </choose>
        <out>
          <lu>
            <clip pos="1" side="tl" part="whole"/>
          </lu>
          <b pos="1"/>
          <lu>
            <clip pos="2" side="tl" part="whole"/>
          </lu>
        </out>
      </action>
    </rule>

    <rule comment=""> 
      <pattern>
        <pattern-item n="default"/>
        <pattern-item n="clit"/>
      </pattern>
      <action>
        <call-macro n="fixup1"><with-param pos="1"/></call-macro>
        <call-macro n="fixup1"><with-param pos="2"/></call-macro>
        <out>
          <mlu>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
            <lu><clip pos="2" side="tl" part="whole"/></lu>
          </mlu>
        </out>
      </action>
    </rule>

    <rule comment=""> 
      <pattern>
        <pattern-item n="default"/>
        <pattern-item n="clit"/>
        <pattern-item n="clit-tag"/>
      </pattern>
      <action>
        <call-macro n="fixup1"><with-param pos="1"/></call-macro>
        <call-macro n="fixup1"><with-param pos="2"/></call-macro>
        <call-macro n="fixup1"><with-param pos="3"/></call-macro>
        <out>
          <mlu>
            <lu><clip pos="1" side="tl" part="whole"/></lu>
            <lu><clip pos="2" side="tl" part="whole"/><clip pos="3" side="tl" part="a_foc"/></lu>
          </mlu>
        </out>
      </action>
    </rule>


    <rule comment=""> 
      <pattern>
        <pattern-item n="default"/>
        <pattern-item n="clit-tag"/>
      </pattern>
      <action>
        <call-macro n="fixup1"><with-param pos="1"/></call-macro>
        <out>
          <lu><clip pos="1" side="tl" part="whole"/><clip pos="2" side="tl" part="a_foc"/></lu>
        </out>
      </action>
    </rule>
      
  </section-rules>
