      <let><clip pos="2" side="tl" part="a_sem"/><lit v=""/></let>
      <let><clip pos="3" side="tl" part="a_sem"/><lit v=""/></let>
    </def-macro>


    <def-macro n="default-conversion1" npar="1">
      <let><clip pos="1" side="tl" part="a_func"/><lit v=""/></let>
      <choose>
        <when>
          <test>
                     <or>
                     <equal><clip pos="1" side="sl" part="a_func"/><lit-tag v="@←SUBJ"/></equal>
                     <equal><clip pos="1" side="sl" part="a_func"/><lit-tag v="@SUBJ→"/></equal></or></test>
          <let><clip pos="1" side="sl" part="a_func"/><lit-tag v="@SUBJ"/></let>
        </when>
      </choose>

      <choose>
        <when>
          <test><and><equal><clip pos="1" side="tl" part="a_cas"/><lit-tag v="par"/></equal>
                     <not><equal><clip pos="1" side="sl" part="a_func"/><lit-tag v="@SUBJ"/></equal></not></and>
          </test>
          <let><clip pos="1" side="tl" part="a_cas"/><lit-tag v="gen"/></let>
        </when>
        <when>
          <test><and><equal><clip pos="1" side="tl" part="a_cas"/><lit-tag v="par"/></equal>
                     <equal><clip pos="1" side="sl" part="a_func"/><lit-tag v="@SUBJ"/></equal></and></test>
          <let><clip pos="1" side="tl" part="a_cas"/><lit-tag v="nom"/></let>
        </when>
        <when>
          <test><or><equal><clip pos="1" side="tl" part="a_cas"/><lit-tag v="ine"/></equal>
                    <equal><clip pos="1" side="tl" part="a_cas"/><lit-tag v="ade"/></equal>
	            <equal><clip pos="1" side="tl" part="a_cas"/><lit-tag v="abl"/></equal>
		    <equal><clip pos="1" side="tl" part="a_cas"/><lit-tag v="ela"/></equal>
                </or></test>
          <let><clip pos="1" side="tl" part="a_cas"/><lit-tag v="loc"/></let>
        </when>
        <when>
          <test><equal><clip pos="1" side="tl" part="a_cas"/><lit-tag v="all"/></equal></test>
          <let><clip pos="1" side="tl" part="a_cas"/><lit-tag v="ill"/></let>
        </when>
      </choose>
      <choose>
        <when>
          <test><equal><clip pos="1" side="tl" part="a_foc"/><lit-tag v="foc_pa"/></equal></test>
          <let><clip pos="1" side="tl" part="a_foc"/><lit-tag v="foc_ba"/></let>
        </when>
      </choose>
      <choose>
        <when>
          <test><equal><clip pos="1" side="sl" part="a_cas"/><lit-tag v="ess"/></equal></test>
          <let><clip pos="1" side="tl" part="a_nbr"/><lit v=""/></let>
        </when>
      </choose>
      <choose>
        <when>
          <test>
	    <and>
	      <equal><clip pos="1" side="sl" part="a_foc"/><lit-tag v="foc_pa"/></equal>
	      <equal><clip pos="1" side="tl" part="a_foc"/><lit-tag v="foc_pa"/></equal>
	    </and>
	  </test>
          <let><clip pos="1" side="tl" part="a_foc"/><lit-tag v="foc_ba"/></let>
        </when>
      </choose>

    </def-macro>

    <def-macro n="default-conversion2" npar="2">
      <call-macro n="default-conversion1"><with-param pos="1"/></call-macro>
      <call-macro n="default-conversion1"><with-param pos="2"/></call-macro>
    </def-macro>

    <def-macro n="default-conversion3" npar="3">
      <call-macro n="default-conversion1"><with-param pos="3"/></call-macro>
      <call-macro n="default-conversion1"><with-param pos="2"/></call-macro>
      <call-macro n="default-conversion1"><with-param pos="1"/></call-macro>
    </def-macro>

    
    
